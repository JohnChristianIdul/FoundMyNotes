<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".studyPage"
    android:background="@drawable/study">

    <Button
        android:id="@+id/btnHome1"
        android:layout_width="92dp"
        android:layout_height="93dp"
        android:alpha="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.059"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnFlashCards"
        app:layout_constraintVertical_bias="0.992" />

    <Button
        android:id="@+id/btnStudy1"
        android:layout_width="92dp"
        android:layout_height="93dp"
        android:layout_marginTop="132dp"
        android:alpha="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnReport1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toEndOf="@+id/btnHome1"
        app:layout_constraintTop_toBottomOf="@+id/btnFlashCards"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btnReport1"
        android:layout_width="86dp"
        android:layout_height="92dp"
        android:layout_marginTop="128dp"
        android:alpha="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnProfile1"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnFlashCards"
        app:layout_constraintVertical_bias="1.0" />

    <Button
        android:id="@+id/btnProfile1"
        android:layout_width="92dp"
        android:layout_height="93dp"
        android:alpha="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.918"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnFlashCards"
        app:layout_constraintVertical_bias="0.992" />

    <Button
        android:id="@+id/btnReminders"
        android:layout_width="267dp"
        android:layout_height="146dp"
        android:layout_marginTop="20dp"
        android:alpha="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnNotes" />

    <Button
        android:id="@+id/btnFlashCards"
        android:layout_width="267dp"
        android:layout_height="146dp"
        android:layout_marginTop="12dp"
        android:alpha="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.562"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnReminders"
        app:layout_constraintVertical_bias="0.0" />

    <Button
        android:id="@+id/btnNotes"
        android:layout_width="267dp"
        android:layout_height="146dp"
        android:layout_marginTop="192dp"
        android:alpha="0"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.562"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>