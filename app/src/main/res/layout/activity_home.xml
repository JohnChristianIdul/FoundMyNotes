<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/home"
    tools:context=".Home">

    <Button
        android:id="@+id/btnHome"
        android:layout_width="84dp"
        android:layout_height="90dp"
        android:layout_marginEnd="56dp"
        android:alpha="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnStudy"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/btnStudy"
        android:layout_width="84dp"
        android:layout_height="90dp"
        android:layout_marginEnd="16dp"
        android:alpha="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnReport" />

    <Button
        android:id="@+id/btnReport"
        android:layout_width="84dp"
        android:layout_height="90dp"
        android:layout_marginEnd="20dp"
        android:alpha="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/btnProfile" />

    <Button
        android:id="@+id/btnProfile"
        android:layout_width="84dp"
        android:layout_height="90dp"
        android:layout_marginEnd="4dp"
        android:alpha="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="150dp"
        android:layout_height="223dp"
        android:background="@drawable/roundedrectangle"
        android:clipToOutline="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.076"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.149"
        app:srcCompat="@drawable/lost_and_found_button"
        tools:clickable="true" />

    <ImageView
        android:id="@+id/imgNotes"
        android:layout_width="151dp"
        android:layout_height="223dp"
        android:layout_marginStart="44dp"
        android:layout_marginEnd="24dp"
        android:background="@drawable/roundedrectangle"
        android:clipToOutline="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/imageView"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.148"
        app:srcCompat="@drawable/notes_and_flashcards_button"
        tools:clickable="true" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="151dp"
        android:layout_height="221dp"
        android:layout_marginStart="105dp"
        android:layout_marginTop="320dp"
        android:layout_marginEnd="104dp"
        android:layout_marginBottom="209dp"
        android:background="@drawable/roundedrectangle"
        android:clipToOutline="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/study_sessions_button"
        tools:clickable="true" />

    <ImageView
        android:id="@+id/imageOver"
        android:layout_width="117dp"
        android:layout_height="62dp"
        app:srcCompat="@drawable/button_container"
        tools:layout_editor_absoluteX="202dp"
        tools:layout_editor_absoluteY="231dp"
        android:clickable="true"/>

</androidx.constraintlayout.widget.ConstraintLayout>