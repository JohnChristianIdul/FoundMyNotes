<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/foundrport">

    <Button
        android:id="@+id/Image"
        android:layout_width="201dp"
        android:layout_height="176dp"
        android:alpha="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.442"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.839" />

    <Button
        android:id="@+id/btnHome3"
        android:layout_width="84dp"
        android:layout_height="90dp"
        android:alpha="0"
        app:layout_constraintEnd_toStartOf="@+id/btnStudy3"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="639dp" />

    <Button
        android:id="@+id/btnStudy3"
        android:layout_width="84dp"
        android:layout_height="90dp"
        android:layout_marginEnd="12dp"
        android:alpha="0"
        app:layout_constraintEnd_toStartOf="@+id/btnReport3"
        tools:layout_editor_absoluteY="639dp" />

    <Button
        android:id="@+id/btnReport3"
        android:layout_width="84dp"
        android:layout_height="90dp"
        android:alpha="0"
        app:layout_constraintEnd_toStartOf="@+id/btnProfile3"
        tools:layout_editor_absoluteY="639dp" />

    <Button
        android:id="@+id/btnProfile3"
        android:layout_width="84dp"
        android:layout_height="90dp"
        android:layout_marginEnd="32dp"
        android:alpha="0"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/editTextText4"
        android:layout_width="244dp"
        android:layout_height="24dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/foredittext"
        android:clipToOutline="true"
        android:ems="10"
        android:gravity="center"
        android:inputType="text"
        android:outlineProvider="background"
        app:layout_constraintBottom_toTopOf="@+id/editTextText3"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="1.0" />

    <EditText
        android:id="@+id/editTextText"
        android:layout_width="244dp"
        android:layout_height="24dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/foredittext"
        android:clipToOutline="true"
        android:ems="10"
        android:gravity="center"
        android:inputType="text"
        android:outlineProvider="background"
        app:layout_constraintBottom_toTopOf="@+id/image"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/editTextText2"
        android:layout_width="244dp"
        android:layout_height="24dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="36dp"
        android:background="@drawable/foredittext"
        android:clipToOutline="true"
        android:ems="10"
        android:gravity="center"
        android:inputType="text"
        android:outlineProvider="background"
        app:layout_constraintBottom_toTopOf="@+id/editTextText5"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/editTextText3"
        android:layout_width="244dp"
        android:layout_height="24dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="28dp"
        android:background="@drawable/foredittext"
        android:clipToOutline="true"
        android:ems="10"
        android:gravity="center"
        android:inputType="text"
        android:outlineProvider="background"
        app:layout_constraintBottom_toTopOf="@+id/editTextText2"
        app:layout_constraintEnd_toEndOf="parent" />

    <EditText
        android:id="@+id/editTextText5"
        android:layout_width="244dp"
        android:layout_height="24dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="32dp"
        android:background="@drawable/foredittext"
        android:clipToOutline="true"
        android:ems="10"
        android:gravity="center"
        android:inputType="text"
        android:outlineProvider="background"
        app:layout_constraintBottom_toTopOf="@+id/editTextText"
        app:layout_constraintEnd_toEndOf="parent" />

    <ImageView
        android:id="@+id/image"
        android:layout_width="275dp"
        android:layout_height="173dp"
        android:layout_marginBottom="92dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.426"
        app:layout_constraintStart_toStartOf="parent"
        tools:srcCompat="@tools:sample/avatars" />

</androidx.constraintlayout.widget.ConstraintLayout>